{
	"name": "bipi-svc",
	"version": "1.0.0",
	"description": "a barebone CRUD GraphQL backend",
	"main": "src/index.js",
	"scripts": {
		"dev": "nodemon --experimental-specifier-resolution=node src/index.js --exec babel-node -e js",
		"start": "node dist/index.js",
		"prebuild": "rimraf ./dist",
		"build": "npm run prebuild && babel src -d dist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/syukronarie/bipi/tree/master/bipi-svc"
	},
	"keywords": [
		"Nodejs",
		"Expressjs",
		"GraphQL",
		"Knexjs",
		"PostgreSQL"
	],
	"author": {
		"name": "M Arie Syukron ",
		"email": "syukronarie@gmail.com"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/syukronarie/bipi/issues"
	},
	"homepage": "https://github.com/syukronarie/bipi#readme",
	"devDependencies": {
		"@babel/cli": "^7.19.3",
		"@babel/core": "^7.19.3",
		"@babel/node": "^7.19.1",
		"@babel/plugin-transform-runtime": "^7.19.1",
		"@babel/preset-env": "^7.19.3",
		"nodemon": "^2.0.20"
	},
	"dependencies": {
		"apollo-server-core": "^3.10.2",
		"apollo-server-express": "^3.10.2",
		"express": "^4.18.1",
		"graphql": "^16.6.0",
		"rimraf": "^3.0.2"
	}
}
