		"Expressjs",
		"GraphQL",
		"Knexjs",
		"PostgreSQL",
		"Reactjs"
	],
	"author": "M Arie Syukron",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/syukronarie/bipi/issues"
	},
	"homepage": "https://github.com/syukronarie/bipi#readme",
	"devDependencies": {
		"cz-conventional-changelog": "^3.3.0",
		"husky": "^8.0.1"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"dependencies": {
		"concurrently": "^7.4.0"
